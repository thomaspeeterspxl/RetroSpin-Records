{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^5.0.0",
    "elleracompany/craft-cookie-consent": "5.0.1",
    "pierrestoffe/craft-language-redirector": "3.1.1",
    "solspace/craft-freeform": "5.2.2",
    "spacemonk/craft-language-manager": "1.1.0",
    "studioespresso/craft-dumper": "^5.0",
    "studioespresso/craft-navigate": "^5.0",
    "verbb/formie": "3.0.0-beta.10",
    "verbb/hyper": "^2.0@beta",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}